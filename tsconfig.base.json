{
    "compilerOptions": {
      "module": "commonjs",
      "noImplicitAny": false,
      "sourceMap": true,
      "target": "es6",
      "keyofStringsOnly": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "composite": false,
      "declaration": false,
      "declarationMap": false,
      "resolveJsonModule": true,
      "esModuleInterop": true
    },
    "exclude": ["node_modules", "**/*.spec.ts"]
}